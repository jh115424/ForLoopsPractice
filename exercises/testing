function getStringSum(str) {


  const sum = 17;
  const total = 0;





  for(let i = 0; i < str.length; i++){
   let char = str[i];
if(!isNaN(parseInt(char))){
  total += parseInt(char);
}}
return total;

  } 






  console.log(getStringSum("GH2U87A"));
  console.log(getStringSum("GHIUJUHSG"));